   "source": [
    "# příprava\n",
    "\n",
    "# najde všechny soubory v IN složce, aby se mohl loopnout\n",
    "inputs_jpg = os.listdir(\"in/\")\n",
    "inputs = []\n",
    "\n",
    "# vynechám ty s X\n",
    "for a in inputs_jpg:\n",
    "    if a[0] == \"X\":\n",
    "        continue\n",
    "    l = len(a)\n",
    "    # odstraním .jpg\n",
    "    short = a[:l - 4]\n",
