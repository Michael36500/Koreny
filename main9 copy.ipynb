   "source": [
    "# akože distance transform\n",
    "cnt_area = np.array([])\n",
    "for cnt in fitctverec_obvodplocha_kuzel_smaly_nekraj_cnts:\n",
    "    cnt_area = np.append(cnt_area, cv2.contourArea(cnt))\n",
